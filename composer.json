{
  "name": "triquang/laravel-solid-maker",
  "description": "Generate SOLID-based scaffolding (Service, Repository, Tests...) for Laravel — supports both standard and modular architectures.",
  "version": "2.1.1",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyễn Trí Quang",
      "email": "ntquangkk@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0 || ^12.0"
  },
  "autoload": {
    "psr-4": {
      "TriQuang\\LaravelSolidMaker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TriQuang\\LaravelSolidMaker\\LaravelSolidMakerServiceProvider"
      ]
    }
  },
  "keywords": [
    "laravel",
    "module",
    "modular",
    "solid",
    "scaffold",
    "generator",
    "repository",
    "service",
    "interface",
    "ddd"
  ],
  "minimum-stability": "stable",
  "prefer-stable": true
}
