{
  "name": "triquang/laravel-solid-maker",
  "description": "A Laravel package to generate SOLID-structured files (Service, Repository, Interface, Test...)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Nguyễn Trí Quang",
      "email": "ntquangkk@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^11.0||^12.0"
  },
  "autoload": {
    "psr-4": {
      "TriQuang\\LaravelSolidMaker\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TriQuang\\LaravelSolidMaker\\LaravelSolidMakerServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
